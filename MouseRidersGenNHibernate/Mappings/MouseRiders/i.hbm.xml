<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="MouseRidersGenNHibernate"
		                   namespace="MouseRidersGenNHibernate.EN.MouseRiders">
		    
		    
			
				<class name="IEN" table="i"> 
				  	
						<id name="Id" column="idI" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="UsuarioBloqueado" type="string"> 
						<column name="usuarioBloqueado"  not-null="true"/>
					</property>
					<property name="FechaInicio" type="date"> 
						<column name="fechaInicio"  not-null="true"/>
					</property>
					<property name="FechaFin" type="date"> 
						<column name="fechaFin"  not-null="true"/>
					</property>
			
			
			
		  			
						<bag name="Contiene" >
							<key>
							
								
								<column name="FK_idI_idBloqueo"/>
					      	</key>
							<one-to-many class="MouseRidersGenNHibernate.EN.MouseRiders.DenunciaEN"/>
					    </bag>
					
			
		  			
					
							<many-to-one name="Pertenece" class="MouseRidersGenNHibernate.EN.MouseRiders.UsuarioEN" >
								<column name="FK_emailUsuario_idUsuario"/> 
        					</many-to-one>
					 
						
			
			
			
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="IENReadAllHQL">from IEN</query>
		</hibernate-mapping>
	