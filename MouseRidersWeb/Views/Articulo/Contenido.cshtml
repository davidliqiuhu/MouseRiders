@model MouseRidersGenNHibernate.EN.MouseRiders.ArticuloEN

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<header>
    
    <div class="display-field">
        @Html.DisplayFor(model => model.Titulo)
    </div>
   
    <div class="display-field">
