@model MouseRidersGenNHibernate.EN.MouseRiders.ArticuloEN

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<header>
    
    <div class="display-field">
        @Html.DisplayFor(model => model.Titulo)
    </div>
   
    <div class="display-field">
        @Html.DisplayFor(model => model.Subtitulo)
    </div>
    <div class="display-field">
        <img class="featurette-image img-circle img-responsive pull-right" src="@Html.DisplayFor(model => model.Portada)">
    </div>
</header>


