@model MouseRidersWeb.DTO.UsuarioDTO

@{
    ViewBag.Title = "VerMensajesRecibidos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>VerMensajesRecibidos</h2>

<fieldset>
    <legend>UsuarioDTO</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Email)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Email)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Nombre)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Nombre)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Apellidos)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Apellidos)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Pais)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Pais)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Telefono)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Telefono)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Puntuacion)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Puntuacion)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Contrasenya)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Contrasenya)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Nombreusuario)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Nombreusuario)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.FechaRegistro)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FechaRegistro)
    </div>

@foreach (var item in Model.Recibe)
    {
       
            <p>
                @Html.DisplayFor(modelItem => item.Id)
            </p>
            <p>
                @Html.DisplayFor(modelItem => item.Asunto)
            </p>
            <p>
                @Html.DisplayFor(modelItem => item.Texto)
            </p>
            <p>
                @Html.DisplayFor(modelItem => item.Adjunto)
            </p>
    }

</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
